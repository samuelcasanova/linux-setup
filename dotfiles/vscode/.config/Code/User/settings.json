{
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.inlayHints.fontSize": 10,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "security.workspace.trust.untrustedFiles": "open",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.ignoreRecommendations": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.importModuleSpecifier": "relative",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sonarlint.rules": {
    "typescript:S2004": {
      "level": "off"
    },
    "secrets:S6698": {
      "level": "off"
    },
    "kubernetes:S6897": {
      "level": "off"
    },
    "typescript:S2933": {
      "level": "on"
    }
  },
  "sonarlint.pathToNodeExecutable": "/home/samuel/.nvm/versions/node/v22.13.1/bin/node",
  "sonarlint.connectedMode.connections.sonarcloud": [
    {
      "organizationKey": "aviosgroup",
      "connectionId": "aviosgroup"
    }
  ],
  "jest.jestCommandLine": "node_modules/.bin/jest",
  "jest.runMode": "watch",
  "gitlens.views.repositories.showStashes": true,
  "workbench.iconTheme": "material-icon-theme",
  "errorLens.codeLensTemplate": "$severity $message",
  "errorLens.messageTemplate": "$severity $message ($source)",
  "errorLens.gutterIconSet": "circle",
  "editor.linkedEditing": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true
}
